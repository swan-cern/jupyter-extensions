{
  "name": "@swan-cern/swanhelp",
  "version": "1.0.0",
  "description": "Help panel for SWAN",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "SWAN",
    "CERN"
  ],
  "homepage": "https://github.com/swan-cern/jupyter-extensions",
  "bugs": {
    "url": "https://github.com/swan-cern/jupyter-extensions/issues"
  },
  "license": "AGPL-3.0",
  "author": "SWAN Admins",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/swan-cern/jupyter-extensions.git",
    "directory": "SwanHelp"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:all": "jlpm run build:labextension && jlpm run build:nbextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:nbextension": "yarn --cwd nbextension build",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension && jlpm run build:nbextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension && jlpm run clean:nbextension",
    "clean:labextension": "rimraf swanhelp/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:nbextension": "rimraf swanhelp/nbextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:all": "jlpm install && jlpm --cwd nbextension install",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "removeall": "rimraf nbextension/node_modules && rimraf node_modules",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.3",
    "@jupyterlab/apputils": "^3.0.2",
    "@jupyterlab/mainmenu": "^3.0.2",
    "@jupyterlab/services": "^6.0.2",
    "@jupyterlab/ui-components": "^3.0.2",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-rc.13",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "swanhelp/labextension"
  },
  "styleModule": "style/index.js"
}